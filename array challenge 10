import java.util.*;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();
        int zc = 0, oc = 0;

        for (int i = 0; i < n; i++) {
            int val = sc.nextInt();
            if (val == 0) {
                zc++;
            } else if (val == 1) {
                oc++;
            }
        }

        System.out.println("zc = " + zc);
        System.out.println("oc = " + oc);
    }
}
